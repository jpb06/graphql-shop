  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "dev": "nx run-many --target=serve --projects=front-shop-app,back-api-app",
    "docker-db": "docker-compose -f apps/api/docker-compose.yml down && docker-compose -f apps/api/docker-compose.yml up shop-postgres",
    "docker-db-seed": "pnpm prisma-migrate dev && pnpm prisma-seed",
    "prisma-gen": "prisma generate --schema libs/backend/database/model/schema.prisma",
    "prisma-migrate": "prisma migrate dev --schema libs/backend/database/model/schema.prisma --name",
    "prisma-seed": "prisma db seed",
    "sync-api": "pnpm nx fetch graphql-codegen-lib",
    "sync-icons": "generateReadmeIcons",
    "codegen-init": "gqlCodegen-init -c ./libs/graphql/codegen/src -o libs/graphql/artifacts/src/api -u NEXT_PUBLIC_GQL_API_URL",
    "codegen": "rm -rf libs/graphql/artifacts/src/api/codegen && gqlCodegen -c ./libs/graphql/codegen/src/react-query.codegen.yml"
  },
